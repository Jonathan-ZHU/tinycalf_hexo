<!DOCTYPE HTML>
<html>

<head>
	<!-- 百度验证 -->
	<meta name="baidu-site-verification" content="TkwOB1XRD3" />
	<link rel="bookmark"  type="image/x-icon"  href="<%- theme.head.favicon %>"/>
	 <link rel="shortcut icon" href="<%- theme.head.favicon %>">
	<% if(page.layout === 'gallery') { %>
		<%- partial('_partial/gallery_head', {}, {cache: true}) %>
	<% } else { %>
			<%- partial('_partial/head', {}, {cache: true}) %>
			<%- partial('_partial/importJS', {}, {cache: true}) %>
	<% } %>

	<!-- 标签云的样式 -->
	<style type="text/css">
		body {background: #24313d;}
		#tagsList {position: relative;}
		#tagsList a {position: absolute;top: 0px;left: 0px;font-family: Microsoft YaHei;color: #fff;font-weight: bold;text-decoration: none;padding: 3px 6px;}
		#tagsList a:hover {color: #FF0000;letter-spacing: 2px;}
	</style>

</head>
    <% if(page.layout === 'gallery') { %>
        <!-- Single Gallery Page -->
        <%- partial('_widget/page-gallery', {}, {cache: true}) %>

    <% } else { %>
		<%- body %>
 	<% } %>

</html>
