<div id="main" style="background-color: transparent; padding:0;">
<section class="link_container" style="margin-top: 0; background-color: transparent; ">
    <div class="link_row">
<% if (page.prev || page.next){ %>
  <% if (page.prev){ %>
    <div class="6u 12u(narrower)" style='display:inline-block;'>
        <section class="link_box special">
            <a href="<%- url_for(page.prev.path) %>" >
              <div class="page_title" style="background-image: url(<%= page.prev.thumbnail %>);background-position: center; background-repeat:no-repeat; background-size:cover;-moz-background-size:cover;">
                  <!--标题-->
                    <h2 class="page_title_text"> <%= page.prev.title %></h2>
                  <!--段落-->
              </div>
            </a>
            <div class="home-post-info">
                <!-- Author Avatar -->
                <div class="home-post-info-author-avatar">
                    <h6><span class="home-post-info-data"><%- date(page.prev.date, "YYYY-MM-DD") %></span></h6>
                    <div class="home-post-info-Category">
                        <!-- Category -->
                        <span>
                          <h6>
                            <a class="post_category-link" href="http://localhost:4000/categories/Algorithm/"><%- list_categories(page.prev.categories, {
                              show_count: false,
                              class: 'post_category',
                              style: 'none'
                          }) %></a>
                          </h6>
                        </span>
                    </div>
                </div>
            </div>
        </section>
    </div>
  <% } %>

  <% if (!page.prev){ %>
    <div class="6u 12u(narrower)" style='display:inline-block;visibility:hidden'>
      <section class="link_box special">
          <a href="<%- url_for(page.next.path) %>" >
            <div class="page_title" style="background-image: url(<%= page.next.thumbnail %>);background-position: center; background-repeat:no-repeat; background-size:cover;-moz-background-size:cover;">
                <!--标题-->
                  <h2 class="page_title_text"> <%= page.next.title %></h2>
                <!--段落-->
            </div>
          </a>
          <div class="home-post-info">
              <!-- Author Avatar -->
              <div class="home-post-info-author-avatar">
                  <h6><span class="home-post-info-data"><%- date(page.next.date, "YYYY-MM-DD") %></span></h6>
                  <div class="home-post-info-Category">
                      <!-- Category -->
                      <span>
                        <h6>
                          <a class="post_category-link" href="http://localhost:4000/categories/Algorithm/"><%- list_categories(page.next.categories, {
                            show_count: false,
                            class: 'post_category',
                            style: 'none'
                        }) %></a>
                        </h6>
                      </span>
                  </div>
              </div>
          </div>
      </section>
  </div>
  <% } %>

  <% if (page.next){ %>
      <div class="6u 12u(narrower)" style='display:inline-block;'>
        <section class="link_box special">
            <a href="<%- url_for(page.next.path) %>" >
              <div class="page_title" style="background-image: url(<%= page.next.thumbnail %>);background-position: center; background-repeat:no-repeat; background-size:cover;-moz-background-size:cover;">
                  <!--标题-->
                    <h2 class="page_title_text"> <%= page.next.title %></h2>
                  <!--段落-->
              </div>
            </a>
            <div class="home-post-info">
                <!-- Author Avatar -->
                <div class="home-post-info-author-avatar">
                    <h6><span class="home-post-info-data"><%- date(page.next.date, "YYYY-MM-DD") %></span></h6>
                    <div class="home-post-info-Category">
                        <!-- Category -->
                        <span>
                          <h6>
                            <a class="post_category-link" href="http://localhost:4000/categories/Algorithm/"><%- list_categories(page.next.categories, {
                              show_count: false,
                              class: 'post_category',
                              style: 'none'
                          }) %></a>
                          </h6>
                        </span>
                    </div>
                </div>
            </div>
        </section>
    </div>
  <% } %>
<% } %>
</div></section></div>
